<script>
    $("#div1").waterfall({
        itemClass: ".box",
        minColCount: 2,
        spacingHeight: 10,
        resizeable: true,
        ajaxCallback: function (success, end) {
            var data = {
                "data": [
                    {"src": "img1.jpg","title":"11111"}, {"src": "img2.jpg","title":"22222"}, {"src": "img3.jpg","title":"3333"}, {"src": "img4.jpg","title":"4444"}
                ]
            };
            var str = "";
            // var templ = '<div class="box" style="opacity:0;filter:alpha(opacity=0);"><div class="pic"><img src="../images/{{src}}" /><p class="kuai_title"> '+ 'imgTitle' + '</p></div></div>'

            for (var i = 0; i < data.data.length; i++) {
                // str += templ.replace("{{src}}", data.data[i].src);
                str +=  '<div class="box" style="opacity:0;filter:alpha(opacity=0);"><div class="pic"><img src=\"../images/'+ data.data[i].src +'\"/></div><p class="kuai_title">'+ data.data[i].title +'</p></div>'
            }
            $(str).appendTo($("#div1"));
            success();
            end();
        }
    });
    $(document).on("mouseenter",".box",function(){
        var topNum =  parseInt($(this).css("top"));
        topNum1 = topNum - 10;
        $(this).css("top",topNum1+"px")
    })
    $(document).on("mouseleave",".box",function(){
        var topNum =  parseInt($(this).css("top"));
        topNum2 = topNum +10;
        $(this).css("top",topNum2+"px")
    })
</script>
